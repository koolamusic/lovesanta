{
  "name": "with-chakra-ui-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint:ts": "tsc --pretty --noEmit",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "format": "prettier -w src",
    "format:check": "prettier -c src",
    "push-release": "git push --follow-tags origin main"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.7.2",
    "@chakra-ui/theme-tools": "1.3.1",
    "@emotion/react": "11",
    "@emotion/styled": "11",
    "airtable": "^0.10.1",
    "axios": "^0.21.0",
    "chakra-ui-autocomplete": "^1.2.2",
    "downshift": "^6.0.6",
    "framer-motion": "4",
    "js-base64": "^3.7.2",
    "lodash": "^4.17.20",
    "match-sorter": "^6.0.1",
    "next": "^10.0.3",
    "nprogress": "^0.2.0",
    "react": "^17.0.1",
    "react-awesome-reveal": "^3.5.1",
    "react-dom": "^17.0.1",
    "react-highlight-words": "^0.16.0",
    "react-use": "^15.3.4",
    "use-realm": "1.1.3",
    "zustand": "^3.6.6"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.6.0",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "typescript": "4.0.5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier -w"
    ]
  },
  "license": "MIT"
}
